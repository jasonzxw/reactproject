{
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/" ,
        "sourceMap": true ,
        "noImplicitAny": true ,
        "esModuleInterop": true,
        "module": "commonjs" ,
        "target": "es5" ,
        "jsx": "react" ,
        "paths": {
            "@/*": [ "src/*" ]
          },
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "public"
      ]
}